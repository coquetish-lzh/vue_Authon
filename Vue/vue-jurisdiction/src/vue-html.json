	"el-menu": {
		"prefix": "el-menu",
		"description": "创建下拉菜单",
		"body": [
			"<el-dropdown trigger=\"click\">",
			"  <span class=\"iconfont icon-more\"> </span>",
			"  <template #dropdown>",
			"    <el-dropdown-menu>",
			"      <el-dropdown-item @click=\"\" >菜单选项</el-dropdown-item>",
			"    </el-dropdown-menu>",
			"  </template>",
			"</el-dropdown>"
		]
